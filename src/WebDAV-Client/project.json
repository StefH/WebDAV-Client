{
    "version": "1.0.0.0",
    "title": "WebDAV-Client",
    "description": "An easy-to-use async WebDAV client for .NETStandard, Portable and .NET 4.5 and up",
    "authors": [ "Sergey Kazantsev", "Stef Heyenrath" ],

    "packOptions": {
        "summary": "An easy-to-use async WebDAV client for .NETStandard, Portable and .NET 4.5 and up",
        "tags": [ "webdav", "client", "NETStandard", ".NET", "Portable" ],
        "owners": [ "Stef Heyenrath" ],
        "repository": {
            "type": "git",
            "url": "https://github.com/stefh/WebDAV-Client"
        },
        "projectUrl": "https://github.com/stefh/WebDAV-Client",
        "licenseUrl": "https://raw.githubusercontent.com/stefh/WebDavClient/master/LICENSE.txt",
        "releaseNotes": "Added NETStandard 1.1 and Portable"
    },

    "dependencies": {
        "JetBrains.Annotations": {
            "version": "10.2.1",
            "type": "build"
        }
    },

    "frameworks": {
        "net45": {
            "frameworkAssemblies": {
                "System.Net.Http": { "type": "build" },
                "System.Xml": { "type": "build" },
                "System.Xml.Linq": { "type": "build" }
            }
        },
        "netstandard1.1": {
            "buildOptions": { "define": [ "NETSTANDARD" ] },
            "dependencies": {
                "System.Collections": "4.3.0",
                "System.Linq": "4.3.0",
                "System.Runtime": "4.3.0",
                "System.Runtime.Extensions": "4.3.0",
                "System.Text.RegularExpressions": "4.3.0",
                "System.Net.Http": "4.3.0",
                "System.Xml.XDocument": "4.3.0"
            }
        },
        ".NETPortable,Version=v4.5,Profile=Profile111": {
            "buildOptions": { "define": [ "PORTABLE" ] },
            "frameworkAssemblies": {
                "Microsoft.CSharp": { "type": "build" },
                "System": { "type": "build" },
                "System.Collections": { "type": "build" },
                "System.IO": { "type": "build" },
                "System.Linq": { "type": "build" },
                "System.Net.Http": { "type": "build" },
                "System.Net.Primitives": { "type": "build" },
                "System.Runtime": { "type": "build" },
                "System.Runtime.Extensions": { "type": "build" },
                "System.Text.Encoding": { "type": "build" },
                "System.Text.RegularExpressions": { "type": "build" },
                "System.Threading.Tasks": { "type": "build" },
                "System.Xml.XDocument": { "type": "build" },
                "System.Xml.ReaderWriter": { "type": "build" }
            }
        },
        ".NETPortable,Version=v4.6,Profile=Profile151": {
            "buildOptions": { "define": [ "PORTABLE" ] },
            "frameworkAssemblies": {
                "Microsoft.CSharp": { "type": "build" },
                "System": { "type": "build" },
                "System.Collections": { "type": "build" },
                "System.IO": { "type": "build" },
                "System.Linq": { "type": "build" },
                "System.Net.Http": { "type": "build" },
                "System.Net.Primitives": { "type": "build" },
                "System.Runtime": { "type": "build" },
                "System.Runtime.Extensions": { "type": "build" },
                "System.Text.Encoding": { "type": "build" },
                "System.Text.RegularExpressions": { "type": "build" },
                "System.Threading.Tasks": { "type": "build" },
                "System.Xml.XDocument": { "type": "build" },
                "System.Xml.ReaderWriter": { "type": "build" }
            }
        }
    }
}